<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff2d2c2c" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#fffefefd" />
            <corners android:radius="4.0dip" />
            <stroke android:width="1.0px" android:color="#99ffffff" />
        </shape>
    </item>
</selector>