<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <shape>
            <solid android:color="#ffb3ecbc" />
            <corners android:radius="99.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_checked="false">
        <shape>
            <solid android:color="#fffefefe" />
            <corners android:radius="99.0dip" />
            <stroke android:width="1.5dip" android:color="#fff0f0f0" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <solid android:color="#ff43d95d" />
            <corners android:radius="99.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#fffefefe" />
            <corners android:radius="99.0dip" />
            <stroke android:width="1.5dip" android:color="#ffe6e6e6" />
        </shape>
    </item>
</selector>