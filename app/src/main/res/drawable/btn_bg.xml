<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#fff0f6ff" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1.0px" android:color="#ffc6c7c8" />
            <solid android:color="#fffefefd" />
        </shape>
    </item>
</selector>